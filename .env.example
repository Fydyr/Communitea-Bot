# Docker Configuration (pour le déploiement)
NAME=discord-bot

# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_GUILD_ID=your_guild_id_here

# Channels
STATUS_CHANNEL_ID=your_status_channel_id_here
ANECDOTE_CHANNEL_ID=your_anecdote_channel_id_here

# Webhook pour les logs (optionnel, recommandé pour les logs Discord)
LOG_WEBHOOK_URL=your_webhook_url_here

# API Keys
GEMINI_API_KEY=your_gemini_api_key_here
